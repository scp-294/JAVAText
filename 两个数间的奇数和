package text1;

import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner a = new Scanner(System.in);
		int num[] = new int[2];
		int t, i, ans=0;
		for(i=0; i<2; i++)		
			num[i] = a.nextInt();
		if(num[0]>num[1])
		{
			t = num[1];
			num[1] = num[0];
			num[0] = t;
		}
		t=num[0];
		if(num[0]%2==0)
			num[0]+=1;
		else
			num[0]+=2;
		
		for(i=num[0]; i<num[1]; i+=2)
				ans+=i;
		System.out.println(t+"和"+num[1]+"之间的奇数和为"+ans);
	}
	
}
